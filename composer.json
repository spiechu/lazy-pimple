{
    "name": "spiechu/lazy-pimple",
    "description": "Lazy service definitions for Pimple DI container.",
    "type": "library",
    "license": "MIT",
    "keywords": ["pimple", "dic", "lazy"],
    "homepage": "https://github.com/spiechu/lazy-pimple",
    "authors": [
        {
            "name": "Dawid Spiechowicz",
            "email": "spiechu@gmail.com",
            "homepage": "http://spiechu.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ocramius/proxy-manager": "~1.0",
        "pimple/pimple": "~3.0",
        "symfony/event-dispatcher": "~2.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Spiechu\\LazyPimple\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiechu\\LazyPimple\\": "test"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "config": {
            "bin-dir": "bin"
        }
    },
    "minimum-stability": "stable"
}
